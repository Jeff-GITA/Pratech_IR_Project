    "        \n",
    "        n_path = os.path.join(n_path, dirs[i])\n",
    "        # print(n_path)  \n",
    "        if(not(os.path.isdir(n_path)) and (n_path != \"\")):\n",
    "            os.mkdir(n_path)\n",
    "\n",
